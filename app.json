{
  "expo": {
    "name": "CarbClean - Mobile CARB Testing",
    "slug": "carbclean-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "description": "The #1 mobile CARB compliance testing app for Northern California truckers. Book tests with OCR, track compliance, 24/7 AI support.",
    "primaryColor": "#9BC53D",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#9BC53D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carbapple.v3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#9BC53D"
      },
      "package": "com.carbapple.v3"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "CARB",
      "shortName": "CARB",
      "description": "Green carbon tracking for truckers",
      "themeColor": "#9BC53D",
      "backgroundColor": "#ffffff",
      "meta": {
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "black-translucent"
      }
    }
  }
}
